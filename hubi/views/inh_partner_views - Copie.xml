                    <page name="etiquette" string="Etiquette" >
                        <group name="etiquette_row" attrs="{'invisible': [('is_company','=',True),('customer_rank','=',0),('supplier_rank','=',0)]}">
                            <group name="etiquette_row_1">
                                <field name="di_customer_name_etiq" />
                                <field name="di_customer_city_etiq" />
                                <field name="di_etiq_model_id" />
                                <field name="di_etiq_printer" />
                                <!--<div attrs="{'invisible': [('di_is_etiq_couleur_client', '=', False)]}">-->
                                <div>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_magenta"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FF00FF')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_blue"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#0000FF')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_yellow"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FFFF00')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_red"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FF0000')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_green"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#008000')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_brown"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#D2691E')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_white"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FFFFFF')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_grey"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#CCCCCC')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_pink"  attrs="{'invisible': [('di_customer_color_etiq', '!=', '#FFC0CB')]}"/>
                                    <label for="di_customer_color_etiq" string="The Color is " class="oe_white"  attrs="{'invisible': [('di_customer_color_etiq', '!=', False)]}"/>
                                </div>
                                <!--<field name="di_customer_color_etiq" nolabel="1" attrs="{'invisible': [('di_is_etiq_couleur_client', '=', False)]}"/>-->
                                <field name="di_customer_color_etiq" nolabel="1" />
                            </group>
                            <group name="etiquette_row_2">
                                <!--
                                <field name="di_automatic_batch"  attrs="{'invisible': [('is_etiq_lot_auto', '=', False)]}" />
                                <field name="di_ean128" attrs="{'invisible': [('is_etiq_ean_128', '=', False)]}" />
                                <field name="di_dlc"  attrs="{'invisible': [('is_etiq_dlc', '=', False)]}"/>
                                <field name="di_dlc_number_day"  attrs="{'invisible': [('is_etiq_dlc', '=', False)]}"/>-->
                                <field name="di_code_ean" />
                                <field name="di_automatic_batch"  />
                                <field name="di_dlc" />
                                <field name="di_dlc_number_day"/>

                            </group>
                        </group>
                        
                        <group name="etiquette_row_establ" attrs="{'invisible': ['|',('customer_rank','!=',0),('supplier_rank','!=',0)]}">
                            <group name="etiquette_row">
                                <field name="di_company_name_etiq" />
                                <field name="di_company_city_etiq" /> 
                                <field name="di_etiq_mention" /> 
                            </group>  
                            <group name="etiquette_row_2">
                                <field name="di_code_ean128" />   
                                <field name="di_compteur_ean128" />
                            </group>
                        </group>
                        
                        <!-->
                        <group name="etiquette_type" attrs="{'invisible': [('is_etiq_type', '=', False)]}">
                            <field name="di_type_etiq" options="{'horizontal': true}" widget="radio"/>
                        </group>
                        -->
                    </page>
                </xpath>

                <xpath expr="//page[@name='etiquette']" position="after">
                    <!-->
                    <page name="EDI" string="EDI" attrs="{'invisible': [('di_is_edi_facture', '=', False),('di_is_edi_transporteur', '=', False)]}">
