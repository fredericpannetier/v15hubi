<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_partner_customer" model="ir.ui.view">
            <field name="name">partner.customer</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field eval="90" name="priority"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="inside">
                    <link rel="stylesheet" href="/hubi/static/src/css/style.css"/>
                </xpath>

                <xpath expr="//field[@name='title']" position="replace"/>
                <!--<xpath expr="//sheet/notebook/page[@name='sales_purchases']/group [@name='container_row_2']/group/field[@name='ref']" position="replace"/>-->

                <xpath expr="//field[@name='parent_id']" position="after">
                    <field name="company_name" attrs="{'invisible': ['|', '|', ('company_name', '=', True), ('company_name', '!=', ''), ('is_company', '=', False)]}"/>
                    <field name="title"/>
                </xpath>

                <xpath expr="//field[@name='vat']" position="replace"/>
                <!-->
                <xpath expr="//field[@name='category_id']" position="after">
                    <field name="cee_code"/>
                </xpath>
                -->
                <xpath expr="//field[@name='category_id']" position="after">
                    <group attrs="{'invisible': [('is_company','=',True),('customer_rank','=',0),('supplier_rank','=',0)]}">
                        <field name="customer_rank" string="Is a customer" widget="boolean"/>
                        <field name="supplier_rank" string="Is a supplier" widget="boolean"/>
                    </group>
                </xpath>
                
    <!-->
                <xpath expr="//field[@name='lang']" position="after">
                    <field name="di_family_type_id" string="Type" context="{'default_main_level': 'Partner', 'default_level': 'Type', 'default_level_partner': 'Type'}"/>
                    <field name="di_family_job_id" string="Job" context="{'default_main_level': 'Partner', 'default_level': 'Job', 'default_level_partner': 'Job'}" />
                    <field name="di_family_id" string="Family" context="{'default_main_level': 'Partner', 'default_level': 'Family', 'default_level_partner': 'Family'}"/>
                </xpath>
    -->
              
                <xpath expr="//page[@name='internal_notes']" position="before">
                    <page name="characteristic" string="Characteristic" attrs="{'invisible': [('is_company','=',True),('customer_rank','=',0),('supplier_rank','=',0)]}">
                        <group name="characteristic_row_1">
                            <group string="Various" name="carac_various">
                                
                                <field name="di_sender_establishment" domain="[('is_company','=',True)]" options="{'no_create': True}" />
                                <!--<field name="carrier_id" />-->
                                <field name="di_excluded_packaging" />
                            </group>
                            <group string="Information" name="information" attrs="{'invisible': [('is_company', '=', False)]}">
                                <!-->
                                <field name="di_health_number" />
                                <field name="di_red_label_number" />
                                <field name="di_cnuf" />
                                <field name="di_siret" />
                                -->
                                <field name="di_asset" />
                                
                                <field name="di_rcs" />
                                <field name="di_naf" />
                                
                            </group>
                            <!-->
                            <group string="FRS - IFLS" name="frs_ifls" attrs="{'invisible': [('di_is_frs', '=', False),('di_is_ifls', '=', False)]}">
                                <field name="di_frs_code" attrs="{'invisible': [('di_is_frs', '=', False)]}"/>
                                <field name="di_ifls_code"  attrs="{'invisible': [('di_is_ifls', '=', False)]}"/>
                                <field name="di_ifls_edit_invoice" attrs="{'invisible': [('di_is_ifls', '=', False)]}"/>
                                <field name="di_ifls_edit_delivery" attrs="{'invisible': [('di_is_ifls', '=', False)]}"/>
                                <field name="di_ifls_edit_etiq" attrs="{'invisible': [('di_is_ifls', '=', False)]}" />
                            </group>
                            -->
                        </group>
                    </page>
                </xpath>

                <xpath expr="//page[@name='characteristic']" position="after">
                    <page name="management" string="Management" attrs="{'invisible': [('is_company','=',True),('customer_rank','=',0),('supplier_rank','=',0)]}">
                        <group name="management_row_1">
                            <group string="Various" name="management_various">
                                <!-->
                                <field name="di_shipping" invisible = "1"/>
                                <field name="di_billing_fees" invisible = "1"/>
                                <field name="di_deb" />
                                -->
                                 <field name="di_invoice_grouping" />
                                <field name="di_periodicity_invoice" />
                            </group>
                            <group string="Discount" name="discount">
                                <field name="di_discount_invoice" />
                                <field name="di_discount_ca" />
                                <field name="di_discount_period_ca" />
                                <field name="di_discount_description" />
                            </group>
                        </group>
                    </page>
                </xpath>

                <xpath expr="//page[@name='management']" position="after">
